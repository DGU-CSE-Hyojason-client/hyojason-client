{
  "expo": {
    "name": "hyojason_client",
    "slug": "native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leejeongwk98.hyojason-client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "googleServiceFile": "./google-services.json"
      },
      "package": "com.leejeongwk98.hyojason_client"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "24245cde-6b87-4079-ab59-7b119300fc86"
      }
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow hyojason to access your microphone",
          "speechRecogntionPermission": "Allow hyojason to securely recognize user speech"
        }
      ]
    ]
  }
}
